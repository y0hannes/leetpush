{
  "manifest_version": 3,
  "name": "LeetPush",
  "version": "1.0",
    "description": "Push LeetCode solutions to GitHub.",
  "permissions": [
    "identity",
    "storage",
    "scripting",
    "tabs",
    "activeTab"
  ],
  "oauth2": {
    "client_id": "Ov23liaXlwEUylxBjzey",
    "scopes": ["repo", "user"]
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
  "*://*.ngrok.io/*",

    "https://github.com/*"
  ]
}

